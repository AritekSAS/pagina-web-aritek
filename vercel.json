{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/wp-admin", "destination": "https://presentaciones.aritek.com.co/wp-admin" },
    { "source": "/wp-admin/:path*", "destination": "https://presentaciones.aritek.com.co/wp-admin/:path*" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" }
      ]
    }
  ]
}




